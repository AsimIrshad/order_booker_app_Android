<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asimirshad.dynamic_row_entry">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"

        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:icon="@drawable/yelp"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Products_Activity" />
        <activity android:name=".Shops_Activity" />
        <activity android:name=".New_Orders_Activity" />
        <activity android:name=".My_Orders_Activity" />
        <activity android:name=".New_product_Activity" />
        <activity android:name=".New_shop_activity" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".Area_activity"
            android:label="@string/title_activity_area_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".OrderList"></activity>
    </application>

</manifest>